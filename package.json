{
  "name": "caldera.dev",
  "version": "0.1.0",
  "private": true,
  "author": "Facalder",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && npm run lint:fix && npm run lint:format",
    "lint:fix": "eslint ./**/* --ext .jsx,.js, --quiet --fix -f mo --ignore-path ./.gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,css,md,json}\"",
    "prepare": "husky install",
    "commit": "commit"
  },
  "dependencies": {
    "autoprefixer": "10.4.14",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "next": "13.3.0",
    "next-themes": "^0.2.1",
    "postcss": "8.4.21",
    "preline": "^1.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.8.0",
    "tailwindcss": "3.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@commitlint/prompt-cli": "^17.6.1",
    "@tailwindcss/typography": "^0.5.9",
    "concurrently": "^8.0.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standart": "^0.2.0",
    "eslint-formatter-mo": "^1.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tailwindcss": "^3.11.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "pnpm run lint"
    ]
  }
}
